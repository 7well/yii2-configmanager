{
    "name": "julatools/yii2-configmanager",
    "description": "Flexible configuartion/parameter manager for Yii2",
    "keywords": ["yii2", "yii2-configmanager", "configuration parameter management", "julatools"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/julatools/yii2-configmanager/issues?state=open",
        "forum": "https://gitter.im/julatools/yii2-configmanager",
        "source": "https://github.com/julatools/yii2-configmanager"
    },
    "authors": [
        {
            "name": "Christian Dumhart",
            "email": "christian.dumhart@chd.at",
            "homepage": "http://www.chd.at"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "julatools/yii2-user": "~0.10"
    },
    "autoload": {
        "psr-4": {
            "julatools\\configmanager\\": ""
        }
    },
    "extra": {
        "bootstrap": "configmanager\\user\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}

